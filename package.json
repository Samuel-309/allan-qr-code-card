{
  "name": "allan-qr-code-card",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080 --open",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "preview:debug": "vite preview --host 0.0.0.0 --port 8080 --debug",
    "serve": "npx http-server dist -p 8080 -c-1",
    "deploy": "npm run build && npx vercel deploy --prod"
  },
  "dependencies": {
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "http-server": "^14.1.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "terser": "^5.46.0",
    "vercel": "^33.0.0",
    "vite": "^4.2.0"
  }
}